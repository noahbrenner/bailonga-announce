{
  "name": "bailonga-announce",
  "description": "Generate the weekly announcements for Bailonga",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "build": "vite build",
    "start": "vite",
    "check-formatting": "prettier --check --ignore-path .gitignore '**/*.{ts,js,json,css}'",
    "lint": "tsc --noEmit && eslint --ignore-path .gitignore ."
  },
  "lint-staged": {
    "*.{ts,js,json,css}": "prettier --write"
  },
  "engines": {
    "node": "18.x.x"
  },
  "browserslist": [
    "last 1 year"
  ],
  "dependencies": {
    "knockout": "~3.5.1"
  },
  "devDependencies": {
    "@types/ejs": "~3.1.2",
    "@types/html-minifier-terser": "~7.0.0",
    "@types/node": "~18.14.2",
    "@types/pug": "~2.0.6",
    "@typescript-eslint/eslint-plugin": "~5.53.0",
    "@typescript-eslint/parser": "~5.53.0",
    "autoprefixer": "~10.4.13",
    "ejs": "~3.1.8",
    "eslint": "~8.34.0",
    "eslint-config-prettier": "~8.6.0",
    "eslint-plugin-import": "~2.27.5",
    "html-minifier-terser": "~7.1.0",
    "husky": "~8.0.3",
    "lint-staged": "~13.1.2",
    "postcss": "~8.4.21",
    "postcss-nesting": "~11.0.1",
    "prettier": "~2.8.4",
    "pug": "~3.0.2",
    "typescript": "~4.9.5",
    "vite": "~4.0.4",
    "vite-plugin-pug-transformer": "~1.0.3"
  },
  "author": "Noah Brenner",
  "license": "MIT",
  "repository": "github:noahbrenner/bailonga-announce",
  "homepage": "https://noahbrenner.github.io/bailonga-announce"
}
