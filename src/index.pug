doctype html
html(lang="en-us")
  head
    title Hello, world!
    meta(charset="utf-8")
    meta(http-equiv="X-UA-Compatible", content="IE=edge")
    meta(name="viewport", content="initial-scale=1")
  body
    h1 Bailonga Templates
    section
      h2 Enter values for this week's event
      form
        label(for="title") Event Title
          input(
            data-bind="textInput: title"
            type="text"
            autocomplete="off"
          )#title
    section
      h2 Copy the results below
      textarea(
        data-bind="textInput: text"
        readonly
        spellcheck="false"
      )
